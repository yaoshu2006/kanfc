/* pages/wfcssq/wfcssq.wxss */

.wan_1 {
    background: #f8f8f8;
    height: auto;
    overflow: hidden;
}
.w_left {
    height: auto;
    font-family: "宋体";
    padding-bottom: 20px;
    padding-top: 10px;
}
.redTitle {
    text-align: left;
    padding: 10px;
}
.w_top {
    height: 45px;
    background: #fff;
}

.w_top h2 view{
    display: inline-block;
}

.w_top h2 .wan_hover {
    color: #ba2636;
    border-bottom: 3px solid #ba2636;
}
.w_top h2 navigator {
    float: left;
    width: 25.0%;
    height: 45px;
    line-height: 45px;
    font-weight: normal;
    text-align: center;
    border-bottom: none;
    font-family: "arial";
}
.redTitle {
    text-align: left;
    padding: 10px;

    font-weight: bold;
    font-size: 30rpx;
    color: #ba2636;
    font-family: "宋体";
}
.redTitle span {
    color: #bbb;
    font-size: 26rpx;
}
.redball {
    height: auto;
    padding: 20px 20px 10px;
    overflow: hidden;
}

.redball view {
    float: left;
    font-size: 14px;
    height: 33px;
    line-height: 33px;
    margin-bottom: 10px;
    margin-right: 10px;
    text-align: center;
    width: 33px;
    cursor: pointer;
    text-decoration: none;
    font-weight: bold;
    color: #c30001;
    font-family: "微软雅黑";
    border: 1px solid #ccc;
    border-radius: 50%;
}

.red1 {
    /* background: url(../../resources/images/bred1.jpg) no-repeat left top !important; */
