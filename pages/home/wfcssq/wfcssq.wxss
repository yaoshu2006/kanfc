}
.w_bt {
    padding: 11px 0 6px;
    text-align: center;
}
.w_bt .sends {
    background: url(http://wap.kanfucai.com/home/images/btn2.png) no-repeat scroll 0 0px;
    cursor: pointer;
    display: inline-block;
    height: 34px;
    width: 137px;
}
.selected_box {
    clear: both;
    padding-left: 10px;
    padding-right: 10px;
    height: auto;
    overflow: hidden;
    padding-top: 20px;
}
.selected_list {
    border: 1px solid #b6b6b6;
}
.selected_list dl {
    border-left: 1px solid #fafafa;
    border-top: 1px solid #f1f1f1;
    height: 135rpx;
    overflow-x: hidden;
    overflow-y: scroll;
    width: 100%;
}
.selected_btnbox {
    padding-left: 10px;
    position: relative;
    padding-top: 10px;
    font-size: 24rpx;
}
.selected_btnbox a {
    text-decoration: none;
    cursor: pointer;
    color: #767676;
}
.selected_btn {
    background: url(http://wap.kanfucai.com/home/images/bg3.jpg) repeat-x left top;
    border: 1px solid #c9c9c9;
    border-radius: 2px;
    display: block;
    height: 26px;
    line-height: 26px;
    margin-bottom: 8px;
    text-align: center;
    width: 96px;
    cursor: pointer;
    float: left;
    margin-right: 5px;
}
.betresult {
    height: 27px;
    line-height: 27px;
    padding: 15px 0 11px;
    text-align: center;
}
.holderWrapLeft, .holderWrapRight {
    text-align: right;
}
.betresult strong {
    color: #ba2636;
    font-family: Verdana;
    margin: 0 3px;
}
.betresult {
    height: 27px;
    line-height: 27px;
    padding: 15px 0 11px;
    text-align: center;
    font-size: 24rpx;
}
.addSubtract {
    border: 1px solid #abadb3;
    display: inline-block;
    height: 20px;
    margin: 0 3px;
    position: relative;
    top: 6px;
    width: 83px;
    z-index: 50;
}
.addSubtract a.subtract, .addSubtract a.subtractDisable {
    background-position: -72px 0;
    border-right: 1px solid #abadb3;
    float: left;
}
.addSubtract a.subtractDisable, .addSubtract a.subtractDisable:hover {
    background-position: -72px -22px;
    cursor: default;
}
.addSubtract a {
    background: url(http://wap.kanfucai.com/home/images/icons1.png) no-repeat 0 0;
    display: inline-block;
    font-family: Simsun;
    font-size: 14px;
    height: 20px;
    outline: 0 none;
    width: 22px;
    cursor: pointer;
}
.addSubtract input {
    border: 0 none;
    float: left;
    height: 20px;
    line-height: 20px;
    padding: 0;
    text-align: center;
    width: 37px;
}
.addSubtract a.add, .addSubtract a.addDisable {
    background-position: -119px 0;
    border-left: 1px solid #abadb3;
    float: right;
}
.addSubtract a {
    background: url(http://wap.kanfucai.com/home/images/icons1.png) no-repeat 0 0;
    display: inline-block;
    font-family: Simsun;
    font-size: 14px;
    height: 20px;
    outline: 0 none;
    width: 22px;
    cursor: pointer;
}
.holderWrapLeft, .holderWrapRight {
    text-align: right;
}
.betresult strong {
    color: #ba2636;
    font-family: Verdana;
    margin: 0 3px;
}
.wan_btns {
    padding: 40rpx 0;
    text-align: center;
}
.wan_btns a {
    background: url(http://wap.kanfucai.com/home/images/wan_btns.png) no-repeat 0 -180px;
    cursor: pointer;
    display: inline-block;
    height: 43px;
    width: 146px;
    
}
